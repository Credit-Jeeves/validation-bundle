<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title>{% block title %}{{ 'project_name'|trans }}{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
    <link href="{{ asset('bundles/core/css/main.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/core/css/linkselect.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/core/css/buttons.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/core/css/zebra.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/core/css/lightbox.css') }}" rel="stylesheet" type="text/css" />
    <script src="{{ asset('bundles/core/js/jquery-1.9.1.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/core/js/jquery.placeholder.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/core/js/jquery.linkselect.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/core/js/checkbox.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/core/js/zebra.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/core/js/lightbox.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/core/js/ReLoader.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/core/js/common.js') }}" type="text/javascript"></script>
    
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    {% block google_api %}{% endblock %}
    <!--[if gte IE 9]>
        <style type="text/css">
            .gradient {
            filter: none;
        }
        </style>
   <![endif]-->
   <!--[if lte IE 8]>
       <link rel="stylesheet" type="text/css" href="{{ asset('bundles/core/css/main-ie-8.css') }}" />
   <![endif]-->
</head>
<body>
    {% block header %}{% endblock %}
    {% include 'CoreBundle:Lightbox:index.html.twig' only %}
<div class="header">
    <div class="site-logo">
        <img alt="" src="{{ asset('bundles/core/images/logo/logo.png') }}">
    </div>
    <div class="info-block">
        {% block info %}
            {% include '::userLogout.html.twig' %}
        {% endblock %}
    </div>
    <div class="clearboth"></div>
    <div class="header-tabs">{% block tabs %}{% endblock %}</div>
</div>
<div class="clearboth"></div>
<div class="section">
<div class="body gradient">
    <div class="content border">{% block content %}{% endblock %}</div>
</div>
</div>
<div class="footer">
    {% block footer %}{% endblock %}
    <div class="experian-logo">
        <span class="experian-data">Data provided by</span><br>
        <img alt="" src="{{ asset('bundles/core/images/logo/experian_sm.png') }}">
    </div>
</div>
{% block javascripts %}{% endblock %}
</body>
</html>
