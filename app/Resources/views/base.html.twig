<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title>{% block title %}CreditJeeves{% endblock %}</title>
    <link href="{{ asset('bundles/core/css/main.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/core/css/tabs.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/core/css/buttons.css') }}" rel="stylesheet" type="text/css" />
    <script src="{{ asset('bundles/core/js/jquery-1.9.1.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/core/js/checkbox.js') }}" type="text/javascript"></script>
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
  </head>
  <body>
    <div class="header">
      <div class="site-logo">
        <img alt="" src="{{ asset('bundles/core/images/logo/logo.png') }}">
      </div>
      <div class="info-block">{% block info %}{% endblock %}</div>
      <div class="clearboth"></div>
      <div class="header-tabs">{% block tabs %}{% endblock %}</div>
    </div>
    <div class="clearboth"></div>
    <div class="body">
      <div class="content border">{% block content %}{% endblock %}</div>
    </div>
    <div class="footer">
      <div class="footer-info"></div>
      <div class="experian-logo">
        <img alt="" src="{{ asset('bundles/core/images/logo/experian_sm.png') }}">
      </div>
    </div>
    {% block javascripts %}{% endblock %}
  </body>
</html>
