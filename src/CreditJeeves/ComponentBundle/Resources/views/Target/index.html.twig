<div class="pod-small pod-border pod-padding shadow vehicle-pod" style="background-image: url('{{ sTargetUrl }}');">
    <h3>{{ 'goods.vehicle.title' | trans }}</h3>
    <div class="vehicle-model">
        <span>{{ sTargetName }}</span>
    {% if sLink | length > 0 %}
    <a href="{{ sLink }}" target="_blank">{{ sLink }}</a>
    {% endif %}
    </div>
    {% if nLeads > 1 %}
        <div id="lead-select-button" class="lead-select-button">{{ 'leads.select.target' | trans }}</div>
    {% endif %}
    <div class="clearboth"></div>
</div>

