<div class="base-form-row background-grey">
    <div class="row-20"></div>
    <h5>{{ 'password.create_new' | trans }}</h5>
    <div class="form-label">
        {{ form_label(form.password_new.Password) }}
    </div>
    <div class="form-widget">
        <div class="form-input">{{ form_widget(form.password_new.Password) }}</div>
    </div>
    <div class="clearboth"></div>
    <div class="row-20"></div>
    <div class="form-label">
        {{ form_label(form.password_new.Retype) }}
    </div>
    <div class="form-widget">
        <div class="form-input">{{ form_widget(form.password_new.Retype) }}</div>
    </div>
    <div class="clearboth"></div>
    <div class="row-20"></div>
</div>
