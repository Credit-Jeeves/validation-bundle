{% if app.user %}
    <div class="user-logout">
        {% if isAdmin %}
            <a href="{{ path('public_back_to_admin') }}">{{ 'back.to.admin'|trans }}</a>&nbsp;|&nbsp;
        {% else %}
            {{ email }}&nbsp;|&nbsp;
        {% endif %}
        <a href="http://renttrack.uservoice.com/" data-uv-trigger>{{ 'common.help' | trans }}</a> &nbsp;|&nbsp;
        {% if accountType %}
            {% if accountType == 'landlord' %}
                <a href="{{ path('landlord_password') }}">{{ 'common.account'|trans }}</a> &nbsp;|&nbsp;
            {% else %}
                <a href="{{ path('user_password') }}">{{ 'common.account'|trans }}</a> &nbsp;|&nbsp;
            {% endif %}
        {% endif %}
        <a href="{{ path('fos_user_security_logout') }}"> {{ 'common.log_out' | trans }}</a>
    </div>
{% else %}
    <div class="user-logout">
        <a href="{{ path('fos_user_security_login') }}">{{ 'login.submit' | trans }}</a>
        &nbsp;|&nbsp;
        <a href="http://renttrack.uservoice.com/">{{ 'common.help' | trans }}</a>
    </div>
{% endif %}
