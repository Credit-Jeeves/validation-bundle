<!-- Resolve Popup -->
<div id="contract-resolve" class="pricing-popup dashboard-popup properties-popup">
            <h1>{{ 'conflict.resolve.title' | trans() }}</h1>
            <div class="clearfix halfs-box">
                <div class="grey-half-box" data-bind="with: details">
                    <h4>{{ 'tenant.info' | trans() }}</h4>
                    <div class="sub-box">
                        <p data-bind="text: tenant"></p>
                        <p data-bind="text: full_address"></p>
                    </div>
                    <div class="sub-box">
                        <p>{{ 'conflict.due.date' | trans() }}:&nbsp;<strong data-bind="text: late_date"></strong></p>
                        <p class="late" data-bind="text: late"></p>
                    </div>
                </div>
                <div class="half-box right">
                    <h4>{{ 'conflict.resolve.options' | trans() }}</h4>
                    <ul class="checkboxes">
                        <li>
                            <label class="checkbox radio">
                                <input type="radio" name="ResolutionOptions" title="email" checked>
                                <i></i>
                                <span>{{ 'conflict.resolve.email' | trans() }}</span>
                            </label>
                        </li>
                        <li>
                            <label class="checkbox radio">
                                <input type="radio" name="ResolutionOptions" title="paid">
                                <i></i>
                                <span>{{ 'conflict.resolve.paid' | trans() }}</span>
                            </label>
                        </li>
                        <li>
                            <label class="checkbox radio">
                                <input type="radio" name="ResolutionOptions" title="unpaid">
                                <i></i>
                                <span>{{ 'conflict.resolve.unpaid' | trans() }}</span>
                            </label>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="clearfix col-box" id="blockPopupEditProperty" style="display: block;">
                <div class="footer-button-box">
                    <button class="button buttonFix" data-bind="click: resolve">
                        <span>{{ 'conflict.resolve.action' | trans() }}</span>
                    </button>
                    <button class="button grey buttonFix" data-bind="click: closeForm">
                        <span>{{ 'cancel' | trans() }}</span>
                    </button>
                </div>
            </div>
        </div>

<!-- End Resolve Popup -->
<!-- ko if: countActions() -->
<div id="actions-block" class="landrord-box contracts-block" style="display:none;">
    <div class="title-box clearfix">
      <h2 data-bind="text: 'Action Required (' + $root.total() + ')'"></h2>
      <div class="fiter-box">
        <label>{{ 'filter.by'|trans }}</label>
        <select class="select custom-select blue-arrow selectBox" style="display: none;">
          <option selected="selected" value="0">- Select One -</option>
        </select>
        <div class="finder-box">
          <input type="text" id="searsh-field" name="searsh-field" placeholder="">
          <button type="submit" id="search-submit" class="pie-el">{{ 'search' }}</button>
        </div>
      </div>
    </div>
    <div class="content-box">
      <div class="properties-table-block table-wrap">
        <div class="table-margin">
          <table class="properties-table zebra-grid">
            <thead>
              <tr class="properties-th">
                <th class="pie-el" colspan="2"><span class="sort">{{ 'filter.status'|trans }}<i> </i></span></th>
                <th class="pie-el"><span class="sort">{{ 'due_date'|trans }}<i> </i></span></th>
                <th class="pie-el"><span class="sort">{{ 'property'|trans }}<i> </i></span></th>
                <th class="pie-el"><span class="sort">{{ 'tenant'|trans }}<i> </i></span></th>
                <th class="pie-el"><span>{{ 'amount'|trans }}</span></th>
                <th class="pie-el"><span class="sort">{{ 'type'|trans }}<i> </i></span></th>
              </tr>
            </thead>
            <tbody>
              <!-- ko foreach: { data: aActions() } -->
              <tr>
                <td class="actions-status late" data-bind="text: status"></td>
                <td class="action-box">
                  <a href="#" class="action-alert pie-el"  data-bind="click: $root.Resolve">
                    <i> </i>{{'resolve'|trans|upper}}
                  </a>
                </td>
                <td data-bind="text: due_day"></td>
                <td data-bind="text: address"></td>
                <td data-bind="text: tenant"></td>
                <td data-bind="text: amount"></td>
                <td class="type-box">
                </td>
              </tr>
              <!-- /ko -->
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <div class="clearboth"></div>
    <!-- ko if: pages().length -->
    <div class="row-20"></div>
    <div class="floatright">
        <!-- ko foreach: pages() -->
            <!-- ko if: $data == $root.current() -->
            <div class="btn grey floatleft" data-bind="click: $root.goToPage, text: $data"></div>
            <!-- /ko -->
            <!-- ko ifnot: $data == $root.current() -->
            <div class="btn green floatleft" data-bind="click: $root.goToPage, text: $data"></div>
            <!-- /ko -->
        <!-- /ko -->
    </div>
    <!-- /ko -->
    <div class="clearboth"></div>
    <div class="row-20"></div>
    <!-- /ko -->
</div>
<!-- /ko -->
