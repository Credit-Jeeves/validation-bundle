<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2006 sp2 U (http://www.altova.com) by Mary Shollenberger (Experian) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" version="5.0">
	<xs:element name="NetConnectRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="EAI">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="8"/>
							<xs:minLength value="8"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DBHost">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="PRECISE_ID"/>
							<xs:enumeration value="PRECISE_ID_TEST"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ReferenceId" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="90"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Request">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Products"/>
						</xs:sequence>
						<xs:attribute name="version" type="xs:string"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Products">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="PreciseIDServer"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="PreciseIDServer">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="KBAAddOn"/>
				<xs:sequence>
					<xs:element ref="XMLVersion"/>
					<xs:choice>
						<xs:sequence>
							<xs:element ref="Subscriber"/>
							<xs:element ref="PrimaryApplicant"/>
							<xs:element ref="AccountType" minOccurs="0"/>
							<xs:element ref="AddOns" minOccurs="0"/>
							<xs:element ref="Verbose" minOccurs="0"/>
							<xs:element ref="Vendor" minOccurs="0"/>
							<xs:element ref="Options"/>
							<xs:element ref="NFD" minOccurs="0"/>
							<xs:element ref="BankingInformation" minOccurs="0"/>
							<xs:element ref="ResidenceInformation" minOccurs="0"/>
							<xs:element ref="TotalIncome" minOccurs="0"/>
							<xs:element name="Dependents" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Primary" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:pattern value="((\d){1,2})?"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element ref="JobHistory" minOccurs="0"/>
							<xs:element ref="CheckPointOptions" minOccurs="0"/>
							<xs:element ref="ReferenceNumber" minOccurs="0"/>
							<xs:element ref="CCNumerics" minOccurs="0"/>
							<xs:element ref="CCStrings" minOccurs="0"/>
							<xs:element ref="IPAddress" minOccurs="0"/>
							<xs:element ref="CreditCardNumbers" minOccurs="0"/>
							<xs:element ref="KBA" minOccurs="0"/>
							<xs:element ref="EarlyWarningOptionalInput" minOccurs="0"/>
							<xs:element ref="DIDOptionalInput" minOccurs="0"/>
						</xs:sequence>
						<xs:element ref="KBAAnswers"/>
					</xs:choice>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="XMLVersion" default="5.0">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:whiteSpace value="collapse"/>
				<xs:pattern value="(5|05|5.0)?"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Subscriber">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Preamble"/>
				<xs:element ref="OpInitials"/>
				<xs:element ref="SubCode"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Preamble">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="([a-zA-Z0-9]{4}){1}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="OpInitials">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="([a-zA-Z0-9]{2}){1}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SubCode">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="([0-9]{7}){1}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="PrimaryApplicant">
		<xs:complexType>
			<xs:all>
				<xs:element ref="Name"/>
				<xs:element ref="SSN" minOccurs="0"/>
				<xs:element ref="CurrentAddress"/>
				<xs:element ref="PreviousAddress" minOccurs="0"/>
				<xs:element ref="DriverLicense" minOccurs="0"/>
				<xs:element ref="Phone" minOccurs="0"/>
				<xs:element ref="Employment" minOccurs="0"/>
				<xs:element ref="Age" minOccurs="0"/>
				<xs:element ref="DOB" minOccurs="0"/>
				<xs:element ref="YOB" minOccurs="0"/>
				<xs:element ref="MothersMaidenName" minOccurs="0"/>
				<xs:element ref="EmailAddress" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="Age">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="([1][4-9]|[2-9][0-9]|10[0-9]|11[0-9]|120)?"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="DOB">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="((0[1-9]|1[0-2])((0[1-9])|(1[0-9])|(2[0-9])|(3[0-1]))((19[0-9][0-9])|(20[0-9][0-9])))?"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="YOB">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="((19[0-9][0-9])|(20[0-9][0-9]))?"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="MothersMaidenName">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="([a-zA-Z \-']){0,60}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="EmailAddress">
		<xs:simpleType>
			<xs:restriction base="xs:string"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Name">
		<xs:complexType>
			<xs:all>
				<xs:element ref="Surname"/>
				<xs:element ref="First"/>
				<xs:element ref="Middle" minOccurs="0"/>
				<xs:element ref="Gen" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="Surname">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="([a-zA-Z \-'\s]{1,65}){1}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="First">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="([a-zA-Z]{1,32}){1}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Middle">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:whiteSpace value="collapse"/>
				<xs:pattern value="([a-zA-Z]{1,32})?"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Gen">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="(SR|JR|(II|2)|(III|3)|(IV|4))?"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SSN">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="(\d{3}-\d{2}-\d{4}|\d{9}|\d{4})?"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CurrentAddress">
		<xs:complexType>
			<xs:all>
				<xs:element name="Street">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="([a-zA-Z0-9# \- / \.]{1,68}){1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="City">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="([a-zA-Z \.]{1,38}){1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="State">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="([a-zA-Z]{2}){1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Zip">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="([\d]{5}([\-]?[\d]{4})?){1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="PreviousAddress">
		<xs:complexType>
			<xs:all>
				<xs:element name="Street" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value="([a-zA-Z0-9# \- / \.]{1,68})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="City" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value="([a-zA-Z \.]{1,38})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="State" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value="([a-zA-Z]{2})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Zip" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value="(([\d]{5}([\-]?[\d]{4})?){1})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="DriverLicense">
		<xs:complexType>
			<xs:all>
				<xs:element name="State" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value="([a-zA-Z]{2})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="Number" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="Number">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:whiteSpace value="collapse"/>
				<xs:pattern value="(([a-zA-Z0-9]{1,21}){1})?"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Phone">
		<xs:complexType>
			<xs:all>
				<xs:element name="Number" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value="([a-zA-Z0-9\(\)\-]{10,13})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Type" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value="[a-zA-Z0-9]{0,1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<!-- CCV Module -->
	<xs:element name="CreditCardNumbers">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CreditCardNumber1" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value='[^"\r\n]*'/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CreditCardType1" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value='([^"\r\n]{1})?'/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CreditCardNumber2" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value='[^"\r\n]*'/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CreditCardType2" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value='([^"\r\n]{1})?'/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CreditCardNumber3" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value='[^"\r\n]*'/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CreditCardType3" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value='([^"\r\n]{1})?'/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- End CCV -->
	<xs:element name="Employment">
		<xs:complexType>
			<xs:all>
				<xs:element ref="Company" minOccurs="0"/>
				<xs:element ref="Address" minOccurs="0"/>
				<xs:element name="City" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value="([a-zA-Z\.\s]{1,11})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="State" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value="([a-zA-Z]{2})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Zip" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:whiteSpace value="collapse"/>
							<xs:pattern value="(([\d]{5}([\-]?[\d]{4})?){1})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="Company">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:whiteSpace value="collapse"/>
				<xs:pattern value="(([A-Za-z0-9\s\-#\.&amp;']{1,23})?){1}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Address">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:whiteSpace value="collapse"/>
				<xs:pattern value="([a-zA-Z0-9# \- \.]{1,25})?"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AccountType">
		<xs:complexType>
			<xs:all>
				<xs:element ref="Type" minOccurs="0"/>
				<xs:element ref="Terms" minOccurs="0"/>
				<xs:element ref="AbbreviatedAmount" minOccurs="0"/>
				<xs:element ref="FullAmount" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="Type">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:whiteSpace value="collapse"/>
				<xs:pattern value="([a-zA-Z0-9]{0,2})"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Terms">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:whiteSpace value="collapse"/>
				<xs:pattern value="[a-zA-Z0-9]{0,3}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AbbreviatedAmount">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:whiteSpace value="collapse"/>
				<xs:pattern value="[0-9]{0,3}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="FullAmount">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:whiteSpace value="collapse"/>
				<xs:pattern value="[0-9]{0,7}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AddOns">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="NewConsumer" minOccurs="0"/>
				<xs:element ref="DirectCheck" minOccurs="0"/>
				<xs:element ref="ProfileSummary" minOccurs="0"/>
				<xs:element ref="RiskModels" minOccurs="0"/>
				<xs:element ref="Demographics" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NewConsumer" type="YesNoType" default="N"/>
	<xs:element name="DirectCheck" type="YesNoType" default="N"/>
	<xs:element name="ProfileSummary" type="YesNoType" default="N"/>
	<xs:element name="RiskModels">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Auto" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="Bankruptcy" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="BankruptcyPLUS" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="BankruptcyWatch" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="CollectScore" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="CreditUnion" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="CrossViewScore" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICO" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICO2" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICO3" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOAdvanced" type="YesNoType" default="N" minOccurs="0"/>
