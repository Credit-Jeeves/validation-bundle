				<xs:element name="FICOAuto" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOAuto2" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOAuto3" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOBank" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOBank2" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOBank3" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOFinance" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOFinance2" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOFinance3" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOInsHomeownerF35" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOInsHomeownerF4" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOInsNonstandardAuto" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOInsPrefAutoGT" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOInsStandardAutoGT" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOInsStandardAutoMin" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOInstall" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOInstall2" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FICOInstall3" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="FraudShield" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="NationalEquiv" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="NationalRisk" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="OldNational" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="RecoveryScoreBank" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="RecoveryScoreRetail" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="Retail" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="ROI1Digit" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="ROI3Digit" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="ScorexPLUS" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="ScorexPLUS2" type="YesNoType" default="N" minOccurs="0"/>
				<xs:element name="SureView" type="YesNoType" default="N" minOccurs="0"/>
