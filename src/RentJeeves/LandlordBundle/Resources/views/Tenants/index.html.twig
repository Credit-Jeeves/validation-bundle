{% extends "LandlordBundle::base.html.twig" %}

{% block content %}
        {{ render(controller('RjComponentBundle:ContractsList:index', {'Group': Group, 'form': form})) }}
{% endblock %}

{% block javascripts %}
    {% javascripts
        '@ComponentBundle/Resources/public/js/groups.js'
        '@RjCoreBundle/Resources/public/js/jquery-ui.js'
        '@RjComponentBundle/Resources/public/js/contract.js'
        '@RjComponentBundle/Resources/public/js/contracts.js'
        '@RjComponentBundle/Resources/public/js/landlordContract.js'
        'bundles/bazingaexposetranslation/js/translator.min.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script type="text/javascript" src="{{ url('bazinga_exposetranslation_js', { 'domain_name': 'messages'}) }}"></script>

    <!-- Call JsFormValidationBundle -->
    {{ JSFV(form) }}
    <!-- END Call JsFormValidationBundle -->
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets '@LandlordBundle/Resources/public/css/tenants.css'%}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}
